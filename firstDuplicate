int firstDuplicate(int[] a) {
    int index;
    int minIndex = a.length+1;
    for (int i=0; i<a.length-1; i++) {
        for (int j=a+1; j<a.length; j++){
            if (i==j) continue;
            else if (a[i]==a[j]) {
                index = j;
                if (j<minIndex)
                    minIndex = j;
            }
        }
    }
    if (minIndex != a.length+1)
        return a[minIndex];
    else return -1;
}
